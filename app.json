{
  "expo": {
    "name": "connectit-app",
    "slug": "connectit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoplash.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoplash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoplash.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gerazayis.connectitapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoplash.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bac3567c-d3cc-42f4-98f0-3d8fea61989e"
      }
    }
  }
}
